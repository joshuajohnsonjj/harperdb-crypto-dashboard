{
  "name": "harperdb-demo",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "type": "module",
  "engines" : { 
    "node" : "20.x"
  },
  "scripts": {
    "start": "node --env-file=.env dist/index.js",
    "start:dev": "NODE_OPTIONS='--loader=tsx' node --env-file=.env src/index.ts",
    "generate:types": "graphql-codegen --config codegen.ts",
    "build:ts": "tsc",
    "postinstall": "npm link harperdb",
    "run:seed": "node --env-file=.env dist/scripts/seed.js",
    "run:live-ingest": "node --env-file=.env src/scripts/liveDatatIngest.ts",
    "build:web:client": "vite build --outDir ./webDist/client",
    "build:web:server": "vite build --ssr src/web/entry-server.js --outDir ./webDist/server",
    "build:web": "npm run build:web:client && npm run build:web:server",
    "format": "prettier --write ."
  },
  "prettier": "@harperdb/code-guidelines/prettier",
  "author": "Joshua Johnson <joshuajohnsonjj38@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.3",
    "@graphql-codegen/typescript": "^4.1.2",
    "@graphql-codegen/typescript-operations": "^4.4.0",
    "@harperdb/code-guidelines": "^0.0.2",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.10.2",
    "graphql": "^16.10.0",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite-plugin-vuetify": "^2.0.4",
    "vue-tsc": "^2.1.10"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "axios": "^1.7.9",
    "lodash": "^4.17.21",
    "technicalindicators": "^3.1.0",
    "vite": "^6.0.5",
    "vue": "^3.5.13",
    "vuetify": "^3.7.6",
    "ws": "^8.18.0"
  }
}
